[advice]
    statusHints = false
[alias]
    amend = commit --amend -C HEAD
    publish = "!git push --set-upstream origin $(git rev-parse --abbrev-ref HEAD)"
    update = pull origin master
    undo = reset --soft HEAD~1
    unstage = reset HEAD --
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = auto
[core]
    excludesfile = ~/.gitexcludes
[push]
    default = tracking
[user]
    name = Diego Oliveira
    email = contato@diegoholiveira.com
[fetch]
    prune = true
[merge]
    tool = vimdiff
