[core]
    excludesfile = ~/dotfiles/git/.gitignore
[user]
    name = Diego Oliveira
    email = contato@diegoholiveira.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = auto
[alias]
    amend = commit --amend -C HEAD
    undo = reset --soft HEAD~1
    branch-clean = "!f() { \
        git branch | grep -v \"master\" | xargs git branch -D; \
        }; f"
	co = checkout
	ci = commit
	br = branch
[push]
    default = tracking

