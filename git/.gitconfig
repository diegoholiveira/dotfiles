[core]
    excludesfile = ~/.gitexcludes
[user]
    name = Diego Oliveira
    email = contato@diegoholiveira.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = auto
[alias]
    amend = commit --amend -C HEAD
    undo = reset --soft HEAD~1
    unstage = reset HEAD --
    publish = "!git push --set-upstream origin $(git rev-parse --abbrev-ref HEAD)"
[push]
    default = tracking
[advice]
    statusHints = false
