alias assume=". assume"
alias cp="cp -iv"
alias icloud="cd ~/Library/Mobile\ Documents/com~apple~CloudDocs/"
alias ls="gls -lahF --group-directories-first --color=auto"
alias mkdir="mkdir -pv"
alias mv="mv -iv"
alias rm="rm -iv"
alias tailf="tail -f"
